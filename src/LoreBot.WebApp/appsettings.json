{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgreSQL": "Host=localhost;Port=5432;Username=postgres;Password=postgres;Database=lore;Pooling=true"
  },
  "Chat": {
    "Provider": "azure-openai",
    "AzureOpenAI": {
      "Endpoint": "not-an-endpoint",
      "Key": "not-a-key",
      "Deployment": "gpt-4o"
    },
    "OpenAI": {
      "ApiKey": "not-a-key",
      "Model": "gpt-4o"
    }
  },
  "Embedding": {
    "Provider": "azure-openai",
    "Dimensions": 768,
    "MaxTokensPerChunk": 2000,
    "TargetTokensPerChunk": 400,
    "OverlapTokens": 75,
    "AzureOpenAI": {
      "Endpoint": "not-an-endpoint",
      "Key": "not-a-key",
      "Deployment": "text-embedding-3-small"
    },
    "OpenAI": {
      "ApiKey": "not-a-key",
      "Model": "text-embedding-3-small"
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "LogPath": "logs/app-.log"
  }
}
